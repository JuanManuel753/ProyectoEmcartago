                <MudRadioGroup @bind-SelectedOption="@Info_registro.licencia_de_conduccion" >
                    <MudRadio Option="@("Bueno")" Color="Color.Primary">Bueno</MudRadio>
                    <MudRadio Option="@("Malo")" Color="Color.Secondary">Malo</MudRadio>
