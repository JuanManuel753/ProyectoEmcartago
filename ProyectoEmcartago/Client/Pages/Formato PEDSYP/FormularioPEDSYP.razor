@inject NavigationManager NavigationManager

<EditForm Model="Info_registro" OnValidSubmit="OnValidSubmit">
    <DataAnnotationsValidator />
    <div style="display:flex; flex-direction:column; overflow: auto; ">
        <div>
        @if(Info_registro.Id != 0)
            {
                <MudButton Style="margin-top:10px; margin-bottom:10px; margin-right:10px;" Variant="Variant.Filled" @onclick="Messageboxvolver" StartIcon="@Icons.Filled.ArrowBack" Color="Color.Info">Volver</MudButton>
                <MudMessageBox @ref="mboxVolver" Title="Precaución!!!" CancelText="Cancelar">
                    <MessageContent>
                        ¿Seguro que desea volver?
                    </MessageContent>
                    <YesButton>
                        <MudButton Variant="Variant.Filled" Color="Color.Warning" OnClick="Volver" StartIcon="@Icons.Material.Filled.ArrowBack">Confirmar</MudButton>
                    </YesButton>
                </MudMessageBox>
            }
            </div>
        <div class="d-flex flex-wrap " style="margin-bottom:30px; width:100%;">
            <MudTextField Style="width:100%;" @bind-Value="@Info_registro.Marca" Label="Marca"  Variant="Variant.Outlined" For="@(() => Info_registro.Marca)"></MudTextField>
            <MudTextField Style="width:100%;" @bind-Value="@Info_registro.Modelo" Label="Modelo" Variant="Variant.Outlined" For="@(() => Info_registro.Modelo)"></MudTextField>
            <MudTextField Style="width:100%;" @bind-Value="@Info_registro.Placa_Vehiculo" Label="Placa vehiculo" Variant="Variant.Outlined" For="@(() => Info_registro.Placa_Vehiculo)"></MudTextField>
            <MudTextField Style="width:100%;" @bind-Value="@Info_registro.Dependencia" Label="Dependecia" Variant="Variant.Outlined" For="@(() => Info_registro.Dependencia)"></MudTextField>
            <MudTextField Style="width:100%;" @bind-Value="@Info_registro.Conductor" Label="Conductor" Variant="Variant.Outlined" For="@(() => Info_registro.Conductor)"></MudTextField>
            
            @if(Info_registro.Id == 0){
               <div style="width:100%; margin-top:20px;" class="d-flex justify-content-start ">
                <input type="date" id="start" name="trip-start" style="width:200px; height:50px; border:1px solid grey; border-radius:5px; padding-right:10px; padding-left:10px;" @bind-value="@Info_registro.Fecha_Revision" disabled="true">
            </div>
            }else{
                <div style="width:100%; margin-top:20px;" class="d-flex justify-content-start ">
                <input type="date" id="start" name="trip-start" style="width:200px; height:50px; border:1px solid grey; border-radius:5px; padding-right:10px; padding-left:10px;" @bind-value="@Info_registro.Fecha_Revision" >
                </div>
                <div style="width:100%; margin-top:20px;" class="d-flex justify-content-start flex-column">
                 @*borrado solo aparece cuando esta editando*@
                 @if (Info_registro.Id != 0)
                    {
                    <MudRadioGroup T="bool" @bind-SelectedOption="Info_registro.Isdelete">
                            <MudRadio Option="false" Color="Color.Success">Guardado</MudRadio>
                            <MudRadio Option="true" Color="Color.Error">Eliminar</MudRadio>
                    </MudRadioGroup>
                        Console.WriteLine(Info_registro.Isdelete);
                    }
                    @*alerta de eliminar*@
                    @if(Info_registro.Isdelete == true)
                    {
                        
                       <MudAlert Icon="@Icons.TwoTone.WarningAmber" Severity="Severity.Error" Dense="true" Style="width:500px;" Class="my-2">Eliminar Seleccionado, Si Guarda El Formato De Permiso Sera Eliminado</MudAlert>     
                    }
            </div>
            }   
        </div>
        @*Documentos del vehiculo*@
        <h2 style="text-align:center;">Documentos</h2>
        <div class="d-flex align-content-center flex-wrap" style="width:100%; border:solid 1px grey; border-radius: 25px; padding:5px; margin-bottom:20px; background:#D6D7E9;">
            <div class="d-flex justify-content-center align-items-center flex-column" style="width:230px; height:220px; margin:10px; border:solid 1px grey; border-radius: 25px; padding:5px;">
